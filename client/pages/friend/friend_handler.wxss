.fd-handle{
  position: fixed;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40px;
  width: 100%;
  background: #ffff
}
.fd-cancel{
  padding-left:5px; 
  width: 50%;
  color: gray;
}
.fd-confirm{
  padding-right:5px; 
  width: 50%;
  color: green;
  text-align: right;
}
.fd-container {
  position: fixed;
  top:40px;
  bottom: 5px;
}
.fd-search {
  height: 50px;
}
.fd-box {
  display: flex;
  flex-direction: row;
  font-size: 14px;
  height: 60px;
  background: #ffff;
  margin: 5px auto;
}
.fd {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.radio{
  padding:0px 5px; 
}
.fd-radio{
  height: 30px;
  width: 30px;
  border-radius: 50%;
  background: #ccc;
}
.radio-selected{
  background: green;
}
.image {
  width: 15%;
}

.image image {
  width: 50px;
  height: 50px;
  /* border-radius: 50%; */
}

.userName {
  padding-left:5px; 
  justify-content: flex-start;
  width: 60%;
}

.userLabel {
  width: 25%;
}

